{
  "name": "@effect-ts/cli",
  "version": "0.4.0",
  "license": "MIT",
  "repository": "https://github.com/Effect-TS/cli.git",
  "homepage": "https://www.matechs.com",
  "scripts": {
    "clean": "yarn ets:rimraf build tsbuildinfo dist",
    "build-fonts-cjs": "copyfiles -f src/figlet/fonts/*.flf dist/figlet/fonts",
    "build-fonts-esm": "copyfiles -f src/figlet/fonts/*.flf dist/_esm/figlet/fonts",
    "build-fonts": "yarn ets:concurrently \"yarn build-fonts-cjs\" \"yarn build-fonts-esm\"",
    "build-cjs": "yarn ets:tsc -p tsconfig.build.cjs.json",
    "build-esm": "yarn ets:tsc -p tsconfig.build.esm.json && babel build/esm/ -d build/esm/ -s --plugins annotate-pure-calls",
    "build-post": "yarn ets:build-utils pack",
    "build": "yarn ets:concurrently \"yarn build-cjs\" \"yarn build-esm\" && yarn build-post && yarn build-fonts",
    "lint": "yarn ets:eslint . --ext .ts,.tsx",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "yarn ets:prettier --write \"./{src,test,demo}/**/*.ts\"",
    "tc": "yarn ets:tsc --noEmit",
    "circular": "yarn ets:madge --circular --ts-config ./tsconfig.json --extensions ts ./src"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "peerDependencies": {
    "@effect-ts/core": "0.48.0",
    "@effect-ts/node": "0.31.0",
    "@effect-ts/printer": "0.11.1"
  },
  "config": {
    "side": [],
    "modules": [
      "Args",
      "AutoCorrect",
      "BuiltInOption",
      "CliApp",
      "CliConfig",
      "Command",
      "CommandDirective",
      "CompletionScript",
      "Completion",
      "Exists",
      "figlet",
      "figlet/client/BuilderAction",
      "figlet/client/ClientOptions/HorizontalLayout",
      "figlet/client/ClientOptions/Justification",
      "figlet/client/ClientOptions/PrintDirection",
      "figlet/client/FigletClient",
      "figlet/client/FontFileReader",
      "figlet/client/OptionsBuilder",
      "figlet/client/RenderOptions",
      "figlet/core/FigCharacter",
      "figlet/core/FigFont",
      "figlet/core/FigFontParameters/HorizontalLayout",
      "figlet/core/FigFontParameters/HorizontalSmushingRule",
      "figlet/core/FigFontParameters/PrintDirection",
      "figlet/core/FigFontParameters/VerticalLayout",
      "figlet/core/FigFontParameters/VerticalSmushingRule",
      "figlet/core/FigFontSettings",
      "figlet/core/FigHeader",
      "figlet/core/FigHeaderParameters/FullLayout",
      "figlet/core/FigHeaderParameters/OldLayout",
      "figlet/core/Figure",
      "figlet/core/SubElements/SubColumns",
      "figlet/core/SubElements/SubLines",
      "figlet/error/FigletException",
      "figlet/rendering",
      "figlet/rendering/HorizontalMergeRules",
      "figlet/rendering/MergeAction",
      "figlet/rendering/MergeStrategy",
      "Help",
      "Internal",
      "Internal/Console",
      "Internal/FileSystem",
      "Internal/NewType",
      "Internal/Transducers",
      "Options",
      "PathType",
      "PrimType",
      "ShellType",
      "UsageSynopsis",
      "Validation"
    ]
  }
}
