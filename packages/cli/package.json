{
  "name": "@effect-ts/cli",
  "version": "0.2.0",
  "license": "MIT",
  "repository": "https://github.com/Effect-TS/cli.git",
  "homepage": "https://www.matechs.com",
  "private": false,
  "scripts": {
    "clean": "rimraf build tsbuildinfo",
    "build:lib": "ttsc --build  tsconfig.build.json && build-utils package",
    "build:es6": "ttsc --build  tsconfig.es6.json && babel build/esm/ -d build/esm/ -s --plugins annotate-pure-calls && build-utils package-esm",
    "build:post-process": "build-utils rewrite-source-maps && build-utils rewrite-locals && yarn prettier:lib",
    "build": "yarn run build:lib && yarn run build:es6 && yarn copy-fonts:es6 && yarn run build:post-process ",
    "copy-fonts:es6": "copyfiles -f src/figlet/fonts/*.flf build/esm/figlet/fonts",
    "lint": "eslint . --ext .ts,.tsx",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "prettier --write \"./{src,test,demo}/**/*.ts\"",
    "prettier:lib": "prettier --write \"./build/**/*.js\"",
    "tc": "tsc --noEmit",
    "demo": "ts-node --compiler ttypescript --project tsconfig.build.json",
    "circular": "madge --circular --ts-config ./tsconfig.build.json --extensions ts ./src"
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "sideEffects": false,
  "peerDependencies": {
    "@effect-ts/core": "^0.45.5",
    "@effect-ts/node": "^0.30.0",
    "@effect-ts/printer": "^0.11.1"
  },
  "config": {
    "side": [],
    "modules": [
      "Args",
      "AutoCorrect",
      "BuiltInOption",
      "CliApp",
      "CliConfig",
      "Command",
      "CommandDirective",
      "Exists",
      "figlet",
      "figlet/client/BuilderAction",
      "figlet/client/ClientOptions/HorizontalLayout",
      "figlet/client/ClientOptions/Justification",
      "figlet/client/ClientOptions/PrintDirection",
      "figlet/client/FigletClient",
      "figlet/client/FontFileReader",
      "figlet/client/OptionsBuilder",
      "figlet/client/RenderOptions",
      "figlet/core/FigCharacter",
      "figlet/core/FigFont",
      "figlet/core/FigFontParameters/HorizontalLayout",
      "figlet/core/FigFontParameters/HorizontalSmushingRule",
      "figlet/core/FigFontParameters/PrintDirection",
      "figlet/core/FigFontParameters/VerticalLayout",
      "figlet/core/FigFontParameters/VerticalSmushingRule",
      "figlet/core/FigFontSettings",
      "figlet/core/FigHeader",
      "figlet/core/FigHeaderParameters/FullLayout",
      "figlet/core/FigHeaderParameters/OldLayout",
      "figlet/core/Figure",
      "figlet/core/SubElements/SubColumns",
      "figlet/core/SubElements/SubLines",
      "figlet/error/FigletException",
      "figlet/rendering",
      "figlet/rendering/HorizontalMergeRules",
      "figlet/rendering/MergeAction",
      "figlet/rendering/MergeStrategy",
      "Help",
      "Internal",
      "Internal/Console",
      "Internal/FileSystem",
      "Internal/NewType",
      "Internal/Transducers",
      "Options",
      "PathType",
      "PrimType",
      "ShellType",
      "UsageSynopsis",
      "Validation"
    ]
  }
}
